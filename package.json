{
  "name": "aiusd-skill",
  "version": "1.0.0",
  "description": "AIUSD trading and account management skill via MCP protocol",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "npm run build && node dist/index.js test",
    "clean": "rm -rf dist node_modules package-lock.json",
    "install-deps": "npm install",
    "package": "npm run build && npm pack",
    "setup": "node skill-init.js",
    "postinstall": "node skill-init.js"
  },
  "bin": {
    "aiusd-skill": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "SKILL.md",
    "build.sh",
    "test-client.sh",
    "package.json",
    "tsconfig.json"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.0",
    "commander": "^12.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["mcp", "aiusd", "trading", "skill", "typescript", "blockchain", "defi"],
  "author": "AIUSD Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aiusd/aiusd-skill.git"
  },
  "homepage": "https://github.com/aiusd/aiusd-skill#readme",
  "bugs": {
    "url": "https://github.com/aiusd/aiusd-skill/issues"
  }
}